{
  "name": "content-management-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "postbuild": "npm run prisma:generate",
    "start": "next start -p 3000",
    "start:standalone": "node .next/standalone/server.js",
    "start:standalone:windows": "set NODE_ENV=production&& node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset --force",
    "production:start": "NODE_ENV=production npm run build && npm start",
    "production:migrate": "NODE_ENV=production npx prisma migrate deploy",
    "clean": "rm -rf .next out dist",
    "analyze": "ANALYZE=true npm run build"
  },
  "keywords": [
    "content-management",
    "education",
    "nextjs",
    "prisma",
    "postgresql",
    "aws-s3",
    "production-ready"
  ],
  "author": "Content Management System",
  "license": "ISC",
  "description": "Professional Content Management System for Educational Institutions",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-presigned-post": "^3.787.0",
    "@prisma/client": "^6.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "mime-types": "^3.0.1",
    "next": "^14.1.0",
    "prisma": "^6.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.2",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.11.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.1.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/content-management-system"
  },
  "homepage": "https://your-domain.com",
  "bugs": {
    "url": "https://github.com/your-repo/content-management-system/issues"
  }
}
